<UserControl x:Class="SecurityManagementSystem.VisitorsInside"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SecurityManagementSystemStyle;component/ControlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <UniformGrid DockPanel.Dock="Left" Columns="9">
        
        <Label  Style="{StaticResource labelStyle}" >House No.</Label>
        <TextBox   Name="houseNoTb"  Style="{StaticResource textboxStyle}" ></TextBox>
        <Label  Style="{StaticResource labelStyle}" >Room No.</Label>
        <TextBox   Name="roomNoTb"  Style="{StaticResource textboxStyle}" ></TextBox>
        <Label  Style="{StaticResource labelStyle}" >Sine In Time</Label>
        <TextBox   Name="signIntimeTb"  Style="{StaticResource textboxStyle}" ></TextBox>
        <ProgressBar Value="50" Height="30"/>
    </UniformGrid>
</UserControl>
